{
  "name": "derrick",
  "version": "0.0.1",
  "description": "A frontend for Columbo",
  "productName": "Derrick",
  "author": "frisch.raphael <frisch.raphael@gmail.com>",
  "private": true,
  "scripts": {
    "lint": "eslint --ext .js,.ts,.vue ./",
    "cypress:ct": "cypress run-ct",
    "cypress:e2e": "cypress run",
    "cypress:open": "concurrently \"quasar dev\" \"cypress open --browser edge\" \"cypress open-ct\"",
    "test:unit:ui": "majestic",
    "cypress:version": "cypress version",
    "cypress:open:ct": "cypress open-ct",
    "test:unit:ci": "cypress run-ct",
    "test:unit:coverage": "jest --coverage",
    "test:config": "jest --showConfig",
    "test:unit:watch": "jest --watch",
    "jest:clear": "jest --clearCache",
    "test:unit:watchAll": "jest --watchAll",
    "serve:test:coverage": "quasar serve test/jest/coverage/lcov-report/ --port 8788",
    "concurrently:dev:jest": "concurrently \"quasar dev\" \"jest --watch\"",
    "test:e2e": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open\"",
    "test:e2e:ci": "cross-env E2E_TEST=true start-test \"quasar dev\" http-get://localhost:8080 \"cypress open-ct\""
  },
  "dependencies": {
    "@cypress/vue": "^3.1.0",
    "@cypress/webpack-dev-server": "^1.8.0",
    "@quasar/extras": "^1.0.0",
    "@vue/test-utils": "^2.0.0-rc.18",
    "@vueuse/core": "^7.5.4",
    "axios": "^0.21.4",
    "core-js": "^3.6.5",
    "iso-639-1": "^2.1.12",
    "pinia": "^2.0.9",
    "qrcode": "^1.5.0",
    "quasar": "^2.0.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.13.14",
    "@pinia/testing": "0.0.9",
    "@quasar/app": "^3.2.9",
    "@quasar/quasar-app-extension-testing": "^2.0.0-beta.3",
    "@quasar/quasar-app-extension-testing-e2e-cypress": "^4.0.0-beta.9",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.7",
    "@types/faker": "^5.5.9",
    "@types/jest": "^26.0.14",
    "@types/jest-when": "^2.7.4",
    "@types/node": "^12.20.21",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "babel-jest": "^26.5.6",
    "cypress": "^8.7.0",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^25.2.2",
    "eslint-plugin-vue": "^7.0.0",
    "faker": "^5.5.3",
    "jest": "^26.5.6",
    "jest-when": "^3.5.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.3.5",
    "vue-jest": "^5.0.0-alpha.10"
  },
